{
  "name": "electron-mail",
  "description": "Unofficial ProtonMail Desktop App",
  "version": "4.9.0",
  "author": "Vladimir Yakovlev <desktop-app@protonmail.ch>",
  "license": "MIT",
  "homepage": "https://github.com/vladimiry/ElectronMail",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladimiry/ElectronMail.git"
  },
  "bugs": {
    "url": "https://github.com/vladimiry/ElectronMail/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "./app/electron-main.js",
  "scripts": {
    "postinstall": "npm-run-all yarn:audit postinstall:patch-package postinstall:remove:prebuild-install postinstall:electron-chromedriver",
    "postinstall:remove:prebuild-install": "rimraf ./node_modules/prebuild-install",
    "postinstall:patch-package": "patch-package --patch-dir ./patches/patch-package",
    "postinstall:electron-chromedriver": "npm-run-all postinstall:electron-chromedriver:remove postinstall:electron-chromedriver:download",
    "postinstall:electron-chromedriver:remove": "rimraf ./node_modules/electron-chromedriver/bin",
    "postinstall:electron-chromedriver:download": "node ./node_modules/electron-chromedriver/download-chromedriver.js",
    "yarn:audit": "yarn audit",
    "app:dist:base": "npm-run-all lint test:electron-main test:web build assets",
    "app:dist": "npm-run-all app:dist:base clean:prebuilds electron-builder:install-app-deps",
    "build": "npm-run-all build:electron-builder-hooks build:electron-main build:electron-preload build:web",
    "build:dev": "cross-env NODE_ENV=development npm-run-all build",
    "build:electron-main": "npm-run-all scripts/code-generating/electron-main build:electron-main:compile",
    "build:electron-main:compile": "yarn webpack:shortcut --config ./webpack-configs/electron-main.ts",
    "build:electron-main:dev": "cross-env NODE_ENV=development npm-run-all build:electron-main",
    "build:electron-main:watch": "cross-env WEBPACK_ENV_WATCH=1 npm-run-all build:electron-main",
    "build:electron-main:watch:dev": "cross-env WEBPACK_ENV_WATCH=1 npm-run-all build:electron-main:dev",
    "build:electron-preload": "npm-run-all build:electron-preload:about build:electron-preload:browser-window build:electron-preload:browser-window-e2e build:electron-preload:database-indexer build:electron-preload:search-in-page-browser-view build:electron-preload:webview-primary",
    "build:electron-preload:dev": "cross-env NODE_ENV=development npm-run-all build:electron-preload",
    "build:electron-preload:about": "yarn webpack:shortcut --config ./webpack-configs/preload/about.ts",
    "build:electron-preload:browser-window": "yarn webpack:shortcut --config ./webpack-configs/preload/browser-window.ts",
    "build:electron-preload:browser-window-e2e": "yarn webpack:shortcut --config ./webpack-configs/preload/browser-window-e2e.ts",
    "build:electron-preload:database-indexer": "yarn webpack:shortcut --config ./webpack-configs/preload/database-indexer.ts",
    "build:electron-preload:search-in-page-browser-view": "yarn webpack:shortcut --config ./webpack-configs/preload/search-in-page-browser-view.ts",
    "build:electron-preload:webview-primary": "yarn webpack:shortcut --config ./webpack-configs/preload/webview-primary.ts",
    "build:web": "npm-run-all build:web:browser-window build:web:about build:web:search",
    "build:web:dev": "cross-env NODE_ENV=development npm-run-all build:web",
    "build:web:about": "yarn webpack:shortcut --config ./webpack-configs/web/about.ts",
    "build:web:about:dev": "cross-env NODE_ENV=development npm-run-all build:web:about",
    "build:web:browser-window": "yarn webpack:shortcut --config ./webpack-configs/web/browser-window.ts",
    "build:web:browser-window:dev": "cross-env NODE_ENV=development npm-run-all build:web:browser-window",
    "build:web:search": "yarn webpack:shortcut --config ./webpack-configs/web/search-in-page-browser-view.ts",
    "build:web:search:dev": "cross-env NODE_ENV=development npm-run-all build:web:search",
    "build:electron-builder-hooks": "yarn webpack:shortcut --config ./webpack-configs/electron-builder-hooks.ts",
    "clean:app": "rimraf ./app",
    "clean:app-dev": "rimraf ./app-dev",
    "clean:output": "rimraf ./output",
    "clean:prebuilds": "npm-run-all clean:prebuilds:keytar clean:prebuilds:sodium-native clean:prebuilds:spellchecker clean:prebuilds:desktop-idle",
    "clean:prebuilds:keytar": "rimraf ./node_modules/keytar/build ./node_modules/keytar/prebuilds",
    "clean:prebuilds:sodium-native": "rimraf ./node_modules/sodium-native/build ./node_modules/sodium-native/prebuilds ./node_modules/sodium-native/lib",
    "clean:prebuilds:spellchecker": "rimraf ./node_modules/spellchecker/build ./node_modules/spellchecker/prebuilds",
    "clean:prebuilds:desktop-idle": "rimraf ./node_modules/desktop-idle/build ./node_modules/desktop-idle/prebuilds",
    "assets": "npm-run-all assets:copy assets:webclient:protonmail",
    "assets:dev": "npm-run-all assets:copy:dev assets:webclient:protonmail:dev",
    "assets:copy": "cpx \"./src/assets/dist/**/*\" ./app/assets",
    "assets:copy:dev": "cpx \"./src/assets/dist/**/*\" ./app-dev/assets",
    "assets:webclient:protonmail": "yarn assets:webclient:protonmail:base ./app/webclient",
    "assets:webclient:protonmail:dev": "yarn assets:webclient:protonmail:base ./app-dev/webclient",
    "assets:webclient:protonmail:base": "yarn ts-node:shortcut ./scripts/prepare-webclient/protonmail.ts",
    "electron-builder:install-app-deps": "yarn electron-builder:shortcut install-app-deps --arch=x64",
    "electron-builder:dir": "yarn electron-builder:shortcut --x64 --dir",
    "electron-builder:dist": "yarn electron-builder:shortcut --x64 --publish never",
    "electron-builder:dist:linux": "yarn electron-builder:shortcut --x64 --publish never --linux appimage snap deb freebsd pacman rpm",
    "electron-builder:dist:linux:appimage": "npm-run-all scripts/electron-builder/build-appimage",
    "electron-builder:dist:linux:snap": "npm-run-all scripts/electron-builder/build-snap",
    "electron-builder:dist:linux:deb": "yarn electron-builder:shortcut --x64 --linux deb",
    "electron-builder:dist:linux:freebsd": "yarn electron-builder:shortcut --x64 --linux freebsd",
    "electron-builder:dist:linux:pacman": "yarn electron-builder:shortcut --x64 --linux pacman",
    "electron-builder:dist:linux:rpm": "yarn electron-builder:shortcut --x64 --linux rpm",
    "lint": "npm-run-all lint:code",
    "lint:code": "npm-run-all lint:ts:eslint",
    "lint:ts:base:eslint": "eslint",
    "lint:ts:eslint": "yarn lint:ts:base:eslint \"./src/**/*.ts\" \"./scripts/**/*.ts\" \"./webpack-configs/**/*.ts\"",
    "start:electron": "electron ./app/electron-main.js",
    "start:electron:dev": "electron ./app-dev/electron-main.js",
    "test:e2e": "cross-env TS_NODE_FILES=true npx --no-install --node-arg=\"--require tsconfig-paths/register --require ts-node/register\" ava \"./src/e2e/**/*.{spec,test}.ts\"",
    "test:electron-main": "cross-env NODE_ENV=test npm-run-all test:electron-main:compiled:clean test:electron-main:compiled:compile test:electron-main:compiled:ava",
    "test:electron-main:compiled:ava": "npx --no-install ava \"./output/__test__/electron-main/src/electron-main/__test__/**/*.{spec,test}.js\"",
    "test:electron-main:compiled:clean": "rimraf ./output/__test__/electron-main",
    "test:electron-main:compiled:compile": "cross-env TS_NODE_FILES=true npx --no-install --node-arg=\"--require tsconfig-paths/register\" ttsc --project ./src/electron-main/__test__/tsconfig.json",
    "test:web": "cross-env NODE_ENV=test TS_NODE_FILES=true npx --no-install --node-arg=\"--require tsconfig-paths/register\" karma start ./src/web/browser-window/test/karma.conf.ts --single-run",
    "scripts/ci/appveyor/download-webclients-artifact": "yarn ts-node:shortcut ./scripts/ci/appveyor/download-webclients-artifact.ts",
    "scripts/ci/print-webclients-dist-paths-pattern": "yarn --silent ts-node:shortcut ./scripts/ci/print-webclients-dist-paths-pattern.ts",
    "scripts/code-generating/electron-main": "yarn ts-node:shortcut ./scripts/code-generating/electron-main.ts",
    "scripts/dist-packages/print-hashes": "yarn ts-node:shortcut ./scripts/dist-packages/print-hashes.ts ./dist",
    "scripts/dist-packages/upload": "yarn ts-node:shortcut ./scripts/dist-packages/upload.ts ./dist ./output",
    "scripts/download-tray-icon-font": "yarn ts-node:shortcut ./scripts/download-tray-icon-font.ts",
    "scripts/electron-builder/build-appimage": "yarn ts-node:shortcut ./scripts/electron-builder/build-appimage.ts",
    "scripts/electron-builder/build-snap": "yarn ts-node:shortcut ./scripts/electron-builder/build-snap.ts",
    "scripts/electron-builder/run-with-default-args": "yarn ts-node:shortcut ./scripts/electron-builder/run-with-default-args.ts",
    "scripts/electron-builder/sequential-dist-linux": "yarn ts-node:shortcut ./scripts/electron-builder/sequential-dist-linux.ts",
    "scripts/transfer": "yarn ts-node:shortcut ./scripts/transfer/index.ts",
    "scripts/yarn-audit/audit": "scripts/yarn-audit/audit.sh",
    "electron-builder:shortcut": "yarn scripts/electron-builder/run-with-default-args",
    "ts-node:shortcut": "cross-env TS_NODE_FILES=true ts-node --require tsconfig-paths/register",
    "webpack:shortcut": "cross-env TS_NODE_FILES=true npx --no-install --node-arg=\"--require tsconfig-paths/register\" webpack"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "files": [
      "./src/**/*.{spec,test}.{ts,js}",
      "./output/__test__/**/*.{spec,test}.{ts,js}"
    ],
    "timeout": "120s",
    "verbose": true
  },
