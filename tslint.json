{
  "extends": [
    "tslint:recommended",
    "./tslint.codelyzer.json",
    "./tslint.tslint-rules-bunch.json",
    "./tslint.tslint-consistent-codestyle.json"
  ],
  "rules": {
    "no-unused-variable": true,
    "max-line-length": [
      true,
      140
    ],
    "prefer-for-of": true,
    "await-promise": true,
    "no-floating-promises": true,
    "no-empty-interface": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "interface-name": false,
    "arrow-parens": [
      true
    ],
    "max-classes-per-file": [
      false
    ],
    "no-empty": false,
    "ban": [
      true,
      "switchMap"
    ]
  }
}
